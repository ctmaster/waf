{
    "name": "ctmaster/waf",
    "description": "Waf Framework",
    "authors": [
        {
            "name": "Dawei Zhang",
            "email": "ctmaster@126.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "rdlowrey/auryn": "~1.0",
        "monolog/monolog": "~1.0",
        "illuminate/validation": "5.1.*",
        "illuminate/database": "5.1.*",
        "illuminate/cache": "5.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
